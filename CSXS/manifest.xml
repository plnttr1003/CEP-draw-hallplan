<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ExtensionManifest xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
				   ExtensionBundleId="com.aphall.example"
				   ExtensionBundleName="Andy Bundle Name"
				   ExtensionBundleVersion="1.0"
				   Version="5.0">
	<ExtensionList>
		<Extension Id="com.aphall.example.andytest"
				   Version="0.1"/>
	</ExtensionList>
	<ExecutionEnvironment>
		<HostList>
			<Host Name="ILST" Port="8307" Version="24.0" />
		</HostList>
		<LocaleList>
			<Locale Code="All"/>
		</LocaleList>
		<RequiredRuntimeList>
			<RequiredRuntime Name="CSXS" Version="5.0"/>
		</RequiredRuntimeList>
	</ExecutionEnvironment>
	<DispatchInfoList>
		<Extension Id="com.aphall.example.andytest">
			<DispatchInfo>
				<Resources>
					<MainPath>./index.html</MainPath>
					<ScriptPath>./jsx/script.jsx</ScriptPath>
				</Resources>
				<UI>
					<Type>Panel</Type>
					<Menu>Схема</Menu>
					<Geometry>
						<Size>
							<Height>350</Height>
							<Width>500</Width>
						</Size>
						<MaxSize>
							<Height>450</Height>
							<Width>600</Width>
						</MaxSize>
						<MinSize>
							<Height>350</Height>
							<Width>325</Width>
						</MinSize>
					</Geometry>
					<Icons>
						<Icon Type="Normal"		 >./icons/icon_light.png</Icon>
						<Icon Type="RollOver"	 >./icons/icon_light_over.png</Icon>
						<Icon Type="DarkNormal"	 >./icons/icon_dark.png</Icon>
						<Icon Type="DarkRollOver">./icons/icon_dark_over.png</Icon>
					</Icons>
				</UI>
			</DispatchInfo>
		</Extension>
	</DispatchInfoList>
</ExtensionManifest>
